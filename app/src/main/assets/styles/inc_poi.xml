                    <symbol src="assets:symbols/money/atm.svg" symbol-color="color:amenity-brown" symbol-percent="80" />
                </m>
                <m v="bureau_de_change">
                    <symbol src="assets:symbols/money/exchange.svg" symbol-color="color:amenity-brown" symbol-percent="80" />
                </m>
                <m v="post_box">
                    <symbol src="assets:symbols/amenity/post_box.svg" symbol-color="color:amenity-brown" symbol-percent="70" />
                </m>
                <m v="shower">
                    <symbol src="assets:symbols/amenity/shower.svg" symbol-color="color:amenity-brown" symbol-percent="80" />
                </m>
            </m>
            <m select="when-matched">
                <m k="fee" v="yes">
                    <symbol src="assets:symbols/fee.svg" symbol-percent="120" z-index="1" />
                </m>
                <m k="wheelchair" v="yes|limited">
                    <symbol src="assets:symbols/wheelchair.svg" symbol-percent="120" z-index="1" />
                </m>
                <m k="wheelchair" v="no">
                    <symbol src="assets:symbols/wheelchair-no.svg" symbol-percent="120" z-index="1" />
                </m>
                <m select="first">
                    <m k="tourism" v="information">
                        <m k="information" v="-|office">
                            <text dy="15" size="12" stroke-width="1.5" use="poi" />
                        </m>
                    </m>
                    <m k="memorial" v="plaque|stone">
                        <text dy="13" size="12" stroke-width="1.5" use="poi" />
                    </m>
                    <m>
                        <text use="poi" />
                    </m>
                </m>
                <m zoom-min="17">
                    <text dy="-16" use="house-number" />
                </m>
            </m>
        </m>
        <!-- house numbers -->
        <m cat="buildings" k="building|building:part" zoom-min="17">
            <m k="addr:housenumber">
                <m k="kind" v="~">
                    <text dy="-14" use="house-name" />
                </m>
                <m k="feature">
                    <text dy="-16" use="house-number" />
                </m>
                <m k="feature" v="~">
                    <text use="house-number" />
                </m>
            </m>
            <m k="addr:housenumber" v="~">
                <m k="kind" v="~">
                    <text use="house-name" />
                </m>
            </m>
        </m>
        <!-- lonely house numbers -->
        <m cat="buildings" k="addr:housenumber" zoom-min="17">
            <m k="feature" v="~">
                <text use="house-number" />
            </m>
        </m>
    </m>
</rendertheme>